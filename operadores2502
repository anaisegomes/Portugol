programa {
  funcao inicio() {
    
    real a, b, soma, sub, mult, div

    escreva("Digite o primeiro numero: ")
    leia(a)


    escreva("Digite o segundo numero: ")
    leia(b)

    soma = a + b /// soma dois valores
    sub = a - b
    mult = a * b 
    div = a / b

    escreva("\nA soma dos numeros e igual a: ", soma)
    escreva("\nA subtracao dos numero e igual a: ", sub)
    escreva("\nA multiplicacao dos numeros e igual a: ", mult)
    escreva("\nA a divisao dos numero e igual a: ", div)


  }
}
